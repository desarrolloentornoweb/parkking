<p id="notice"><%= notice %></p>

<h1>Autenticacions</h1>

<table>
  <thead>
    <tr>
      <th>Correo</th>
      <th>Contrasena</th>
      <th>Tipo usuario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @autenticacions.each do |autenticacion| %>
      <tr>
        <td><%= autenticacion.correo %></td>
        <td><%= autenticacion.contrasena %></td>
        <td><%= autenticacion.tipo_usuario %></td>
        <td><%= link_to 'Show', autenticacion %></td>
        <td><%= link_to 'Edit', edit_autenticacion_path(autenticacion) %></td>
        <td><%= link_to 'Destroy', autenticacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Autenticacion', new_autenticacion_path %>
