<p id="notice"><%= notice %></p>

<h1>Estacionamientos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Departamento</th>
      <th>Provincia</th>
      <th>Distrito</th>
      <th>Direccion</th>
      <th>Latitud</th>
      <th>Longitud</th>
      <th>Lunes</th>
      <th>Martes</th>
      <th>Miercoles</th>
      <th>Jueves</th>
      <th>Viernes</th>
      <th>Sabado</th>
      <th>Domingo</th>
      <th>Hinicio</th>
      <th>Hcierre</th>
      <th>Telf</th>
      <th>Nespac</th>
      <th>Largo</th>
      <th>Ancho</th>
      <th>Alto</th>
      <th>Precio</th>
      <th>Img</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @estacionamientos.each do |estacionamiento| %>
      <tr>
        <td><%= estacionamiento.nombre %></td>
        <td><%= estacionamiento.departamento %></td>
        <td><%= estacionamiento.provincia %></td>
        <td><%= estacionamiento.distrito %></td>
        <td><%= estacionamiento.direccion %></td>
        <td><%= estacionamiento.latitud %></td>
        <td><%= estacionamiento.longitud %></td>
        <td><%= estacionamiento.lunes %></td>
        <td><%= estacionamiento.martes %></td>
        <td><%= estacionamiento.miercoles %></td>
        <td><%= estacionamiento.jueves %></td>
        <td><%= estacionamiento.viernes %></td>
        <td><%= estacionamiento.sabado %></td>
        <td><%= estacionamiento.domingo %></td>
        <td><%= estacionamiento.hinicio %></td>
        <td><%= estacionamiento.hcierre %></td>
        <td><%= estacionamiento.telf %></td>
        <td><%= estacionamiento.nespac %></td>
        <td><%= estacionamiento.largo %></td>
        <td><%= estacionamiento.ancho %></td>
        <td><%= estacionamiento.alto %></td>
        <td><%= estacionamiento.precio %></td>
        <td><%= estacionamiento.img %></td>
        <td><%= link_to 'Show', estacionamiento %></td>
        <td><%= link_to 'Editar registro', edit_estacionamiento_path(estacionamiento) %></td>
        <td><%= link_to 'Eliminar', estacionamiento, method: :delete, data: { confirm: '¿Está usted seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Registrar nuevo estacionamiento', new_estacionamiento_path %>
