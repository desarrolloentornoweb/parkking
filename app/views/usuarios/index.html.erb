<p id="notice"><%= notice %></p>

<h1>Usuarios</h1>

<table>
  <thead>
    <tr>
      <th>Nrodocumento</th>
      <th>Nombres</th>
      <th>Apellidopaterno</th>
      <th>Apellidomaterno</th>
      <th>Fechanacimiento</th>
      <th>Celular</th>
      <th>Sexo</th>
      <th>Direccion</th>
      <th>Autenticacion</th>
      <th>Distrito</th>
      <th>Tipo documento</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @usuarios.each do |usuario| %>
      <tr>
        <td><%= usuario.nroDocumento %></td>
        <td><%= usuario.nombres %></td>
        <td><%= usuario.apellidoPaterno %></td>
        <td><%= usuario.apellidoMaterno %></td>
        <td><%= usuario.fechaNacimiento %></td>
        <td><%= usuario.celular %></td>
        <td><%= usuario.sexo %></td>
        <td><%= usuario.direccion %></td>
        <td><%= usuario.autenticacion %></td>
        <td><%= usuario.distrito %></td>
        <td><%= usuario.tipo_documento %></td>
        <td><%= link_to 'Show', usuario %></td>
        <td><%= link_to 'Edit', edit_usuario_path(usuario) %></td>
        <td><%= link_to 'Destroy', usuario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Usuario', new_usuario_path %>
